{
  "name": "honey-badger-delivery",
  "version": "1.0.0",
  "description": "Gamified digital gift delivery app with AI chatbot motivators",
  "main": "index.js",
  "scripts": {
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install",
    "setup:frontend": "cd frontend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npx react-native start",
    "ios": "cd frontend && npx react-native run-ios",
    "android": "cd frontend && npx react-native run-android",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "react-native",
    "ios",
    "chatbot",
    "gamification",
    "digital-gifts",
    "motivation",
    "fitness",
    "ai"
  ],
  "author": "LewChew",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LewChew/honey-badger-delivery.git"
  }
}